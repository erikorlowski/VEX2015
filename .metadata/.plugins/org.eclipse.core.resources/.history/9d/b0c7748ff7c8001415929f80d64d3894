#include "main.h"

Lift initLift(PantherMotor bottomMotor, PantherMotor middleMotor,
		PantherMotor topMotor, Encoder encoder, int limitSwitch)
{
	Lift newLift = {bottomMotor, middleMotor, topMotor, encoder, limitSwitch};
	return newLift;
}
