#ifndef PICKUP_H_
#define PICKUP_H_

struct Pickup{

	PantherMotor motor;

}typedef Pickup;

void pickupAtSpeed(Pickup pickup, int speed);
void openPickup(Pickup pickup);
void closePickup(Pickup pickup);
void stopPickup(Pickup pickup);

Pickup initPickup(PantherMotor motor);

Pickup pickup;

#endif /* PICKUP_H_ */
