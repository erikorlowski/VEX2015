class Box
{
	int length, width, height;

public:
	Box(int, int, int);
	int getVolume();
};
svdc
Box::Box(int length, int width, int height)
{
	this->length = length;
	this->width = width;
	this->height = height;
}

int Box::getVolume()
{
	return length * width * height;
}
